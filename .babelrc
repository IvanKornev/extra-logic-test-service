{
    "env": {
      "test": {
        "presets": [
          ["@babel/env", {
            "targets": {
              "node": "current"
            }
          }],
          "@babel/preset-react"
        ]
      },
      "development": {
        "presets": [
          ["@babel/env", {
            "modules": false,
            "targets": {
              "browsers": "> 2%",
              "uglify": true
            }
          }],
          "@babel/preset-react"
        ]
      },
      "production": {
        "presets": [
          ["@babel/env", {
            "modules": false,
            "targets": {
              "browsers": "> 2%",
              "uglify": true
            }
          }],
          "@babel/preset-react"
        ]
      }
    }
}